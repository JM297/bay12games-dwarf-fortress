.list-wrapper
  padding 15px
  overflow hidden

.list-item
  margin-bottom 10px
  padding 10px
  p
    margin 0
    text-align justify
  h4
    text-align center
    font-size 18px
    margin: 0 0 5px

.simple-pagination
  ul
    margin 0 0 20px
    padding 0
    list-style none
    text-align center
  li
    display inline-block
    margin-right 5px

.simple-pagination li a, .simple-pagination li span
  color #666
  padding 5px 10px
  text-decoration none
  border 1px solid #EEE
  background-color #FFF
  font-size inherit
  position relative